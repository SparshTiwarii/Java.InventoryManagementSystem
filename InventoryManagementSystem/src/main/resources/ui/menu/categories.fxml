<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="442.0" prefWidth="617.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.main.controller.menu.CategoryController">
    <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <padding>
            <Insets left="5.0" right="5.0"/>
        </padding>
        <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0"
              spacing="5.0" VBox.vgrow="ALWAYS">
            <VBox.margin>
            </VBox.margin>
            <Button fx:id="buttonRefresh" mnemonicParsing="false" text="Refresh Table"/>
            <TextField fx:id="textFieldTitle" promptText="Title"/>
            <Button fx:id="buttonNew" mnemonicParsing="false" text="New"/>
            <Button fx:id="buttonDelete" layoutX="209.0" layoutY="23.0" mnemonicParsing="false" text="Delete"/>
            <Button fx:id="buttonChange" layoutX="209.0" layoutY="23.0" mnemonicParsing="false" text="Change"/>
        </HBox>
        <TableView fx:id="tableView" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="tableColumnId" prefWidth="103.0" text="Id"/>
                <TableColumn fx:id="tableColumnTitle" prefWidth="363.0" text="Title"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </VBox>
</AnchorPane>
