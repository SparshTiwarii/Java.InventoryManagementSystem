<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="629.0" prefWidth="918.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.main.controller.menu.ProductsController">
    <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <padding>
            <Insets left="5.0" right="5.0"/>
        </padding>
        <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0"
              spacing="5.0" VBox.vgrow="ALWAYS">
            <VBox.margin>
            </VBox.margin>
            <padding>
            </padding>
            <Button fx:id="buttonRefreshTable" layoutX="627.0" layoutY="23.0" mnemonicParsing="false"
                    onAction="#OnPress_Button_RefreshTable" text="Refresh Table"/>
            <Button fx:id="buttonRefreshChoice" mnemonicParsing="false" onAction="#OnPress_Button_RefreshChoice"
                    text="Refresh -&gt;"/>
            <ChoiceBox fx:id="choiceBoxCategory" prefHeight="25.0" prefWidth="89.0"/>
            <TextField fx:id="textFieldName" promptText="Name *"/>
            <TextField fx:id="textFieldPrice" layoutX="185.0" layoutY="28.0" prefHeight="25.0"
                       promptText="Price *"/>
            <TextField fx:id="textFieldDescription" layoutX="339.0" layoutY="28.0" promptText="Description"/>
            <Button fx:id="buttonNewProduct" mnemonicParsing="false" onAction="#OnPress_Button_NewProduct"
                    text="New Product"/>
            <Button fx:id="buttonChangeProduct" mnemonicParsing="false" onAction="#OnPress_Button_NewProduct"
                    text="Change Product"/>
        </HBox>
        <TableView fx:id="tableView" prefWidth="614.0" VBox.vgrow="ALWAYS">
            <columns>
                <TableColumn fx:id="tableColumnIdProduct" prefWidth="103.0" text="IdProduct"/>
                <TableColumn fx:id="tableColumnIdCategory" prefWidth="103.0" text="IdCategory"/>
                <TableColumn fx:id="tableColumnName" prefWidth="363.0" text="Name"/>
                <TableColumn fx:id="tableColumnPrice" prefWidth="363.0" text="Price"/>
                <TableColumn fx:id="tableColumnDescription" prefWidth="363.0" text="Description"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </VBox>
</AnchorPane>
